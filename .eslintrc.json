{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "google"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "linebreak-style": ["error", "windows"]
  },
  "ignorePatterns": [
    "node_modules/",
    "jest.config.js"
  ]
}